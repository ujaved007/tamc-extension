{
	"name": "TAMC Extension",
	"description": "Fill out forms with ease",
	"version": "1.0.0",
	"manifest_version": 3,
	"icons": {
		"16": "tamc-logo.png",
		"48": "tamc-logo.png",
		"128": "tamc-logo.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_title": "TAMC Extension",
		"default_icon": "tamc-logo.png"
	},
	"permissions": ["storage", "activeTab", "tabs", "scripting"],
	"options_page": "options.html",
	"background": {
		"service_worker": "backgroundScript.js"
	}
}
